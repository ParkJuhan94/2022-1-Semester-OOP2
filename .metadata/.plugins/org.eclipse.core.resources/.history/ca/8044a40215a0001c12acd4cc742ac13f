
public class Exception {
	public static int divide(int a,int b) {
		if(b == 0) {
			throw new ExceptionPrac();
		}
		return a/b;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		try {
			int res;
			res = divide(3, 2);
			System.out.println(res);
		} catch(ExceptionPrac e) {
			
		}
	}

}
